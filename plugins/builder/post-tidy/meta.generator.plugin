#!/bin/bash


# the argument passed to this plugin from the builder is "${target}.tmp"
target_tmp=$1


if [ ! -f "${target_tmp}" ]; then
    echo "No temp target found"
    exit 1
fi


sed -i 's|<head>|<head>\n    <meta name="generator" content="https://github.com/HedenEnterprises/blog">|' "${target_tmp}"